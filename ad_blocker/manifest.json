{
    "manifest_version":2,
    "name":"Youtube Ad Blocker",
    "description": "Blocks Youtube Ad",
    "version": "1.0.0",

    "content_scripts":[{
        "js":["script.js"],
        "run_at":"document_idle",
        "matches":["<all_urls>"]
    }],
    "permissions": [
        "tabs",
        "activeTab  "
    ]
}